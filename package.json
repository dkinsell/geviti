{
  "name": "geviti",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "train-model": "ts-node -r tsconfig-paths/register --project tsconfig.scripts.json src/scripts/trainModel.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "vercel-build": "npm run db:prod && mkdir -p /tmp/model && dotenv -e .env.production -- prisma db seed && dotenv -e .env.production -- npm run pre-build-train && npm run build",
    "postinstall": "prisma generate",
    "pre-build-train": "NODE_ENV=production ts-node --project tsconfig.scripts.json scripts/preBuildTrain.ts",
    "db:dev": "NODE_ENV=development ts-node --project tsconfig.scripts.json scripts/switchDbSchema.ts && prisma generate && prisma db push",
    "db:prod": "NODE_ENV=production ts-node --project tsconfig.scripts.json scripts/switchDbSchema.ts && dotenv -e .env.production -- prisma generate && dotenv -e .env.production -- prisma db push",
    "seed:prod": "NODE_ENV=production dotenv -e .env.production -- prisma db seed",
    "seed:dev": "NODE_ENV=development dotenv -e .env.local -- prisma db seed",
    "dev:fresh": "npm run db:dev && npm run seed:dev && npm run dev",
    "dev:reset": "rm -f prisma/dev.db && npm run dev:fresh",
    "prod:reset": "npm run db:prod && npm run seed:prod",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run build",
    "start:prod": "NODE_ENV=production dotenv -e .env.production npm start",
    "build:prod": "NODE_ENV=production dotenv -e .env.production npm run build",
    "prod:test": "npm run prod:reset && npm run build:prod && npm run start:prod"
  },
  "prisma": {
    "seed": "ts-node --project prisma/tsconfig.seed.json prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@prisma/client": "^6.5.0",
    "@tanstack/react-query": "^5.72.1",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-cpu": "^4.22.0",
    "@tensorflow/tfjs-backend-webgl": "^4.22.0",
    "@vercel/blob": "^1.0.0",
    "lucide-react": "^0.487.0",
    "next": "15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.55.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-query-devtools": "^5.72.1",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "eslint-config-prettier": "10.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.7.3",
    "prettier": "3.5.3",
    "prisma": "^6.5.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  }
}
