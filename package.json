{
  "name": "geviti",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "train-model": "ts-node -r tsconfig-paths/register --project tsconfig.scripts.json src/scripts/trainModel.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "vercel-build": "mkdir -p /tmp/model && prisma generate && prisma db push && prisma db seed && npm run pre-build-train && npm run build",
    "postinstall": "prisma generate",
    "pre-build-train": "ts-node --project tsconfig.scripts.json scripts/preBuildTrain.ts"
  },
  "prisma": {
    "seed": "ts-node --project prisma/tsconfig.seed.json prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@prisma/client": "^6.5.0",
    "@tanstack/react-query": "^5.72.1",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-cpu": "^4.22.0",
    "@tensorflow/tfjs-backend-webgl": "^4.22.0",
    "lucide-react": "^0.487.0",
    "next": "15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.55.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-query-devtools": "^5.72.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "eslint-config-prettier": "10.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.7.3",
    "prettier": "3.5.3",
    "prisma": "^6.5.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "@tensorflow/tfjs-node": "^4.22.0",
    "tsconfig-paths": "^4.2.0"
  }
}
